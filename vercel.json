{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1",
            "includeFiles": [
                "public/**",
                "config/**",
                "routes/**",
                "resources/views/**",
                "storage/framework/views/**"
            ]
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
